# Production Environment Variables (DO NOT COMMIT TO GIT)
NODE_ENV=production
PORT=5000

# MongoDB Connection
MONGODB_URI=your_mongodb_connection_string

# Spotify API Credentials
SPOTIFY_CLIENT_ID=your_spotify_client_id
SPOTIFY_CLIENT_SECRET=your_spotify_client_secret
SPOTIFY_REDIRECT_URI=https://your-backend-domain.com/auth/callback

# Session Secret (generate a strong random string)
SESSION_SECRET=your_secure_session_secret_here

# Frontend URL (where your React app is deployed)
FRONTEND_URL=https://your-frontend-domain.com

# Backend URL (where your backend API is deployed)
BACKEND_URL=https://your-backend-domain.com